<%= form_with model: warehouse do |f| %>
    <div>
        <%= f.label :name, 'Nome'%>
        <%= f.text_field :name %>
    </div>
    <div>
        <%= f.label :code, 'Código'%>
        <%= f.text_field :code %>
    </div>
    <div>
        <%= f.label :city: 'Cidade'%>
        <%= f.text_field :city %>
    </div>
    <div>
        <%= f.label :area, 'Área'%>
        <%= f.text_field :area %>
    </div>
    <div>
        <%= f.label :address, 'Endereço'%>
        <%= f.text_field :address %>
    </div>
    <div>
        <%= f.label :cep, 'CEP'%>
        <%= f.text_field :cep %>
    </div>
    <div>
        <%= f.label :description, 'Descrição'%>
        <%= f.text_field :description %>
    </div>
    <div>
        <%= f.submit 'Salvar'%>
    </div>
<% end %>